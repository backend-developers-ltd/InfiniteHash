# InfiniteHash Miner Configuration

[bittensor]
# Bittensor network to connect to
network = "finney"
# Network UID for the subnet
netuid = 89

[wallet]
# Wallet name
name = "default"
# Hotkey name
hotkey_name = "default"
# Path to wallet directory
directory = "~/.bittensor/wallets"

[workers]
# Single price multiplier for all workers (e.g., "1.5" means 1.5x base price)
price_multiplier = "1.0"

# Hashrates are expressed in PH.
# 450 TH = 0.45 PH (max worker size in v2 commitments).

# v1 (legacy): list of hashrate values (each entry = 1 worker)
hashrates = ["0.1", "0.2", "0.3"]

# v2 (recommended): hashrate -> worker count (avoids long lists)
# Inline-table form (must be a single line; no trailing comma):
# worker_sizes = { "0.25" = 4, "0.45" = 2 }
#
# Multi-line form (use this for longer configs):
# [workers.worker_sizes]
# "0.25" = 4
# "0.45" = 2
#
# (Alternative spelling also supported: `hashrates` can be a dict for v2)
# hashrates = { "0.25" = 4, "0.45" = 2 }
# [workers.hashrates]
# "0.25" = 4
# "0.45" = 2
